{
  "workingConditions": {
    "reducedField": "logspace(-3,3,100)",
    "electronTemperature": "linspace(0.03, 5, 100)",
    "excitationFrequency": 0,
    "gasPressure": 133.32,
    "gasTemperature": 77,
    "wallTemperature": 390,
    "extTemperature": 300,
    "surfaceSiteDensity": 1e19,
    "electronDensity": 1e19,
    "chamberLength": 1.0,
    "chamberRadius": 1.0,
    "totalSccmInFlow": 1,
    "totalSccmOutFlow": "ensureIsobaric"
  },
  "electronKinetics": {
    "isOn": true,
    "eedfType": "boltzmann",
    "ionizationOperatorType": "usingSDCS",
    "growthModelType": "spatial",
    "includeEECollisions": false,
    "LXCatFiles": ["CO/CO_LXCat.json", "CO/CO_rot_LXCat.json"],
    "gasProperties": {
      "mass": "Databases/masses.txt",
      "fraction": ["CO = 1"],
      "harmonicFrequency": "Databases/harmonicFrequencies.txt",     
      "anharmonicFrequency": "Databases/anharmonicFrequencies.txt",
      "rotationalConstant": "Databases/rotationalConstants.txt",
      "electricQuadrupoleMoment": "Databases/quadrupoleMoment.txt",
      "OPBParameter": "Databases/OPBParameter.txt"
    },
    "stateProperties": {
      "energy": [
        "CO(X^1S^+,v=*) = harmonicOscillatorEnergy",
        "CO(X^1S^+,v=0,J=*) = rigidRotorEnergy"
      ],
      "statisticalWeight": [
        "CO(X^1S^+) = 1.0",
        "CO(X^1S^+,v=*) = 1.0",
        "CO(X^1S^+,v=0,J=*) = rotationalDegeneracy",
        "CO(a^3P) = 6"
      ],
      "population": [
        "CO(X^1S^+) = 1.0",
        "CO(X^1S^+,v=*) = boltzmannPopulation@gasTemperature",
        "CO(X^1S^+,v=0,J=*) = boltzmannPopulation@gasTemperature"
      ]
    },
    "numerics": {
      "energyGrid": {
        "maxEnergy": 1,
        "cellNumber": 1000,
        "smartGrid": {
            "minEedfDecay": 20,
            "maxEedfDecay": 25,
            "updateFactor": 0.05
        }
      },
      "maxPowerBalanceRelError": 1e-9,
      "nonLinearRoutines": {
        "algorithm": "mixingDirectSolutions",
        "mixingParameter": 0.7,
        "maxEedfRelError": 1e-9
      }
    }
  },
  "chemistry": {
    "isOn": false
  },
  "gui": {
    "isOn": true,
    "refreshFrequency": 1
  },
  "output": {
    "isOn": false,
    "dataFormat":  "hdf5+txt",
    "folder": "COSwarm_77K",
    "dataSets": [
      "log",
      "eedf",
      "swarmParameters",
      "rateCoefficients",
      "powerBalance",
      "lookUpTable"
    ]
  }
}
